<div class="pd-20 card-box mb-30">
    <div class="clearfix">
        <div class="pull-left">
            <h4 class="text-blue h4">Default Basic Forms</h4>
            <p class="mb-30">All bootstrap element classies</p>
        </div>
        <div class="pull-right">
            <a href="#basic-form1" class="btn btn-primary btn-sm scroll-click" rel="content-y" data-toggle="collapse" role="button"><i class="fa fa-code"></i> Source Code</a>
        </div>
    </div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <form>
            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">User Name</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="uName" name="uName" type="text" placeholder="Enter Name">
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">User Number</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="uNumber" name="uNumber" placeholder="Enter Number" type="number">
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">User AlterNumber</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="uAlterNumber" name="uAlterNumber" type="number">
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Email</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="uMail" name="uMail" type="email">
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">PickUpTime</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control time-picker" id="PickUpTime" name="PickUpTime" type="date">
                </div>
            </div>

            <div class="form-group row">
                <label for="example-datetime-local-input" class="col-sm-12 col-md-2 col-form-label">Deliver Date</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control datetimepicker" id="deliverDate" name="deliverDate" placeholder="Choose Date anf time" type="text">
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">PickUp Date</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control date-picker" id="PickUpDate" name="PickUpDate" placeholder="Select Date" type="text">
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">deliverAddres</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="deliverAddres" name="deliverAddres" placeholder="Enter Delevery address" type="text">
                </div>
            </div>

            <div class="form-group">
                <label>Custom file input</label>
                <div class="custom-file">
                    <input type="file" id="ImagePath" name="ImagePath" class="custom-file-input">
                    <label class="custom-file-label">Choose file</label>
                </div>
            </div>

            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Product Type</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="pType" name="pType" placeholder="Enter Delevery address" type="text">
                </div>
            </div>

            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Product Name</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="pName" name="pName" placeholder="Enter Delevery address" type="text">
                </div>
            </div>

            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Product Description</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="pDescription" name="pDescription" placeholder="Enter Delevery address" type="text">
                </div>
            </div>

            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Product Price</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="pPrice" name="pPrice" placeholder="Enter Delevery address" type="text">
                </div>
            </div>

            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Product MRP Price</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="pMrpPrice" name="pMrpPrice" placeholder="Enter Delevery address" type="text">
                </div>
            </div>

            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Deliver Charge</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="delivercharge" name="delivercharge" placeholder="Enter Delevery address" type="text">
                </div>
            </div>


            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Product Offers</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="pOffers" name="pOffers" placeholder="Enter Delevery address" type="text">
                </div>
            </div>


            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Product Specifications</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="pSpecifications" name="pSpecifications" placeholder="Enter Delevery address" type="text">
                </div>
            </div>

            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Order Type</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" id="orderType" name="orderType" placeholder="Enter Delevery address" type="text">
                </div>
            </div>

            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Order Status</label>
                <div class="col-sm-12 col-md-10">
                    <select class="custom-select col-12" id="OrderStatus" name="OrderStatus">
                        <option selected="">Choose...</option>
                        <option value="New Order">New Order</option>
                        <option value="On Going">OnGoing</option>
                        <option value="Completed">Completed</option>
                        <option value="Delivery">Delivery</option>
                    </select>
                </div>
            </div>

            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Order Date</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control date-picker" id="orderDate" name="orderDate" placeholder="Select Date" type="text">
                </div>
            </div>
            <input class="form-control" value="Submit" type="submit" />

        </form>
    }

</div>
<script src="~/vendors/scripts/core.js"></script>
<script src="~/vendors/scripts/script.min.js"></script>
<script src="~/vendors/scripts/process.js"></script>
<script src="~/vendors/scripts/layout-settings.js"></script>
<script src="~/src/plugins/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/src/plugins/datatables/js/dataTables.bootstrap4.min.js"></script>
<script src="~/src/plugins/datatables/js/dataTables.responsive.min.js"></script>
